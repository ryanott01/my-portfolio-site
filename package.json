{
	"name": "skeleton-template-portfolio",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"postbuild": "npx ts-node-esm ./scripts/generate-sitemap.ts",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"dep": "vercel build && vercel deploy --prebuilt",
		"prod": "vercel build --prod && vercel deploy --prebuilt --prod"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.1.0",
		"@skeletonlabs/tw-plugin": "^0.2.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.25.0",
		"@types/node": "^18.16.16",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.33.2",
		"postcss": "^8.4.30",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-plugin-tailwind-purgecss": "^0.1.3"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
		"directory-tree": "^3.5.1"
	}
}
